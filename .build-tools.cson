          stderr:
            pipeline: []
          output:
            console:
              close_success: true
              queue_in_buffer: true
              colors: true
              stdin: true
          version: 2
        }
        {
          project: "/home/divensi/Projects/CG/reader"
          source: "/home/divensi/Projects/CG/reader/.build-tools.cson"
          name: "Clang Build"
          command: "clang++ main.cpp -lGL -lGLU -lglut -o main -lSOIL -std=c++11"
