providers: [
  {
    key: "bt"
    config:
      commands: [
        {
          project: "/home/divensi/Projects/CG/reader"
          source: "/home/divensi/Projects/CG/reader/.build-tools.cson"
          name: "Run "
          command: "gnome-terminal -e ./%e"
